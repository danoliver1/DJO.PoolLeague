@{
    Script.Require("RankLeague").AtFoot();
    Style.Require("PoolLeague");
}

<div class="league-row">
    <span class="rank">
    </span>
    <span class="display-name">
        @Model.Part.DisplayName
    </span>
    <span>
        @Model.Part.WinStreak
    </span>
    <span>
        @Model.Part.WinPercentage.ToString("P1")
    </span>    
    <span>
        @Model.Part.Points
    </span>
    <span>
        <a href="~/PoolLeague/Details?id=@Model.Part.Id">Details</a>
    </span>
</div>